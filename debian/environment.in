#!/bin/bash

export MONODEVELOP_PREFIX=/opt/monodevelop-roslyn-%SNAPVER%
export PATH=$MONODEVELOP_PREFIX/bin:$PATH
export DYLD_LIBRARY_FALLBACK_PATH=$MONODEVELOP_PREFIX/lib:$DYLD_LIBRARY_FALLBACK_PATH
export LD_LIBRARY_PATH=$MONODEVELOP_PREFIX/lib:$LD_LIBRARY_PATH
export C_INCLUDE_PATH=$MONODEVELOP_PREFIX/include:/usr/include:$C_INCLUDE_PATH
export ACLOCAL_PATH=$MONODEVELOP_PREFIX/share/aclocal:$ACLOCAL_PATH
export PKG_CONFIG_PATH=$MONODEVELOP_PREFIX/lib/pkgconfig:$PKG_CONFIG_PATH:/usr/lib/pkgconfig:/usr/share/pkgconfig
export PS1="[monodevelop-roslyn-%SNAPVER%]$PS1"
